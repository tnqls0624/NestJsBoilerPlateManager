{
  "name": "nestjs-boiler-plate",
  "version": "0.0.1",
  "description": "NestJs  Boiler-Plate V3",
  "author": "LeeSooBeen",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "main": "src/main.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "config:dev": "cp ./src/config/env/.dev.env .env && mkdir -p dist && cp ./src/config/env/.dev.env ./dist/.env",
    "config:prod": "cp ./src/config/env/.prod.env .env && mkdir -p dist && cp ./src/config/env/.prod.env ./dist/.env",
    "build:dev": "nest build && npm run config:dev",
    "build:prod": "nest build && npm run config:prod",
    "start": "nest start",
    "start:dev-backup": "nest start --watch",
    "start:dev": "npm run config:dev && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "npm run config:dev && nest start --debug --watch",
    "start:prod": "npm run config:prod && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "eslint": "eslint \"./src/**/*.ts\" && tsc --noemit"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@types/request-ip": "^0.0.37",
    "app-root-path": "^3.1.0",
    "axios": "^1.2.6",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "google-auth-library": "^8.7.0",
    "handlebars": "^4.7.7",
    "joi": "^17.7.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "mysql2": "^3.1.0",
    "nanoid": "^3.3.4",
    "nest-winston": "^1.8.0",
    "nodemailer": "^6.9.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pm2": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^4.1.2",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.16",
    "@types/jest": "29.4.0",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^18.11.18",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,json,css,scss,md}": [
      "eslint"
    ]
  }
}
