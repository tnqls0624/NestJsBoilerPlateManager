{
  "name": "miniscore-crawler",
  "version": "1.0.1",
  "description": "NestJs  Boiler-Plate V3",
  "author": "LeeSooBeen",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "config:local": "cp ./src/config/env/.local.env .env && mkdir -p dist && cp ./src/config/env/.local.env ./dist/.env",
    "config:dev": "cp ./src/config/env/.dev.env .env && mkdir -p dist && cp ./src/config/env/.dev.env ./dist/.env",
    "config:prod": "cp ./src/config/env/.prod.env .env && mkdir -p dist && cp ./src/config/env/.prod.env ./dist/.env",
    "build:dev": "nest build && npm run config:dev",
    "build:prod": "nest build && npm run config:prod",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev-backup": "nest start --watch",
    "start:local": "npm run config:local && rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:dev": "npm run config:dev && rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "npm run config:dev && nest start --debug --watch",
    "start:prod": "npm run config:prod && rimraf dist && nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "lint": "gts lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "clean": "gts clean",
    "eslint": "eslint \"./src/**/*.ts\" && tsc --noemit",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "husky install",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "@fastify/static": "^6.9.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.2",
    "@nestjs/platform-fastify": "^9.3.8",
    "@nestjs/swagger": "^6.1.2",
    "app-root-path": "^3.1.0",
    "aws-sdk": "^2.1357.0",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "fastify": "^4.13.0",
    "husky": "^8.0.3",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "nest-winston": "^1.7.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.17",
    "typescript": "^4.9.5",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.3",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "29.0.3",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gts": "^3.1.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
